  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylee0325/js-code.git"
  },
  "keywords": [
    "javascript",
    "function",
    "tools",
    "utils",
    "typescript"
  ],
  "author": "kylee0325",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylee0325/js-code/issues"
  },
  "homepage": "https://github.com/kylee0325/js-code#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rollup": "^2.77.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^4.7.4",
    "vitepress": "^1.0.0-alpha.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
