{
  "name": "js-code",
  "version": "0.0.1",
  "description": "javascript function collections",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
